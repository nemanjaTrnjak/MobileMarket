<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Failure</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/secured/admin.xhtml</to-view-id>
            <redirect>true</redirect>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/registracija.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Failure</from-outcome>
            <to-view-id>/registracija.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secured/kupovina.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Failure</from-outcome>
            <to-view-id>/secured/kupovina.xhtml</to-view-id>
         </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secured/dodajMobilni.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/secured/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Failure</from-outcome>
            <to-view-id>/secured/dodajMobilni.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secured/dodajServis.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/secured/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Failure</from-outcome>
            <to-view-id>/secured/dodajServis.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secured/dodajKorisnika.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/secured/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Failure</from-outcome>
            <to-view-id>/secured/dodajKorisnika.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secured/dodajOpremu.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/secured/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Failure</from-outcome>
            <to-view-id>/secured/dodajOpremu.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secured/editKorisnik.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/secured/admin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secured/editOprema.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/secured/admin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secured/editMobilni.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/secured/admin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pitanje.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Failure</from-outcome>
            <to-view-id>/pitanje.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/secured/odgovor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Success</from-outcome>
            <to-view-id>/secured/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Failure</from-outcome>
            <to-view-id>/secured/odgovor.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
